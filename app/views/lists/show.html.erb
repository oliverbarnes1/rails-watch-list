<h1><strong><%= @list.name %></strong></h1>

<% @list.bookmarks.each do |bookmark| %>
  <div class="card-trip">
    <img src="<%= bookmark.movie.poster_url %>" alt="">
      <h2><%= bookmark.movie.title %></h2>
      <p><%= bookmark.movie.overview %></p>
      <p>Rating: <strong><%= bookmark.movie.rating %>/10</strong></p>
  </div>
<% end %>
<p><%= link_to "creat bookmark", new_list_bookmark_path(@list) %></p>

<p><%= link_to "back", lists_path %></p>
